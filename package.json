    "url": "https://github.com/skotniczny/mapa/issues"
  },
  "homepage": "https://github.com/skotniczny/mapa#readme",
  "devDependencies": {
    "eslint": "^9.32.0",
    "gh-pages": "^6.3.0",
    "parcel": "^2.15.4",
    "standard": "^17.1.2"
  }
}
